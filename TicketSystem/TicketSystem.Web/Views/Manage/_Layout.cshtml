@{ 
    Layout = "/Views/Shared/_Layout.cshtml";
}

<h2 class="pt-5 prof-heading">Manage your account</h2>

<div>
    <h5 class="prof-heading pb-3">Change your account settings</h5>
    <hr />
    <div class="row" id="prof-layout">
        <div class="col-md-3">
            @await Html.PartialAsync("_ManageNav")
        </div>
        <div class="col-md-9">
            @RenderBody()
        </div>
    </div>
</div>
<script>
    $("main").attr("id","profile");
    $("body").addClass("profile");
</script>

@section Scripts {
    @RenderSection("Scripts", required: false)
}
        
